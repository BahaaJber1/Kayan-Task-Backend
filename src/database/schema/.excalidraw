      "x": 136.14414353357972,
      "y": -189.38037986625665,
      "width": 704.1279750900802,
      "height": 600,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
